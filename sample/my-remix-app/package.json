{
  "name": "remix-azure-functions",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "swa start ./public --run \"remix dev\" --api-location .",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@derkoe/remix-azure-functions": "1.7.1",
    "@prisma/client": "^4.3.1",
    "@remix-run/react": "1.7.1",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.16",
    "prisma": "^4.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@azure/functions": "3.2.0",
    "@azure/static-web-apps-cli": "1.0.2",
    "@remix-run/dev": "1.7.1",
    "@remix-run/eslint-config": "1.7.1",
    "@remix-run/serve": "1.7.1",
    "azure-functions-core-tools": "4.0.4785",
    "concurrently": "^7.4.0",
    "eslint": "8.23.1",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=14"
  }
}
